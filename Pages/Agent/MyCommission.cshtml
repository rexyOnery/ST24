@page
@model ST24.Pages.Agent.MyCommissionModel
@{
    ViewData["Title"] = "My Commission";
}
<!-- Page Title -->
<section class="page-title" style="background-image: url(/assets/images/background/page-title.jpg);">
    <div class="auto-container">
        <div class="content-box">
            <div class="title">
                <h1>Commission</h1>
            </div>
            <ul class="bread-crumb clearfix">
                <li><a href="/agent/dashboard">Home</a></li>
                <li>Commission</li>
            </ul>
        </div>
    </div>
</section>
<!-- End Page Title -->
<section class="pricing-section">
    <div class="map-layer" style="background-image: url(/assets/images/shape/map-1.png);"></div>
    <div class="auto-container">
        <div class="sec-title centred">

            <p>Steezz Transaction - @DateTime.Now.Year</p>
            <h3>Total Generated: <span id="grand_total"></span></h3>
            <img src="/assets/images/icons/icon-1.png" alt="">

            <h3>Commission Earned: <span id="commission_total"></span></h3>
            <img src="/assets/images/icons/icon-1.png" alt="">
        </div>
        <div class="pricing-inner centred">
            <div class="row clearfix">
                <div class='col-lg-12 col-md-6 col-sm-12 pricing-block' id="loading">
                    <h1>Loading...</h1>
                </div>
                <div class='col-lg-3 col-md-6 col-sm-12 pricing-block' id="jan">
                    <div class='pricing-block-one wow fadeInUp animated animated' data-wow-delay='00ms' data-wow-duration='1500ms'>

                    </div>
                </div>

                <div class='col-lg-6 col-md-6 col-sm-12 pricing-block'>
                    <div class='pricing-block-one wow fadeInUp animated animated' data-wow-delay='00ms' data-wow-duration='1500ms'>
                        <div class='icon-box'><i class='flaticon-save-money'></i></div>
                        <div class='inner-box' id="commissiona">
                            <span>Place Request</span>
                            <h3 id="feb_total"></h3>
                            <div class='link' id="place_request_btn">Place Request<a href='javascript:placeRequest();'><i class='fas fa-angle-right'></i></a></div>
                            <div class='link hidden' id="request_placed">Your Request Has Been Placed!</div>
                            <div class='light-icon'><i class='flaticon-save-money'></i></div>
                        </div>
                        <div class='inner-box hidden' id="comm_bank">
                            <input type="hidden" id="commission_id" />
                            <input class="p-2" type="text" id="txtBank" placeholder="Enter Bank Name" /><br /><br />
                            <input class="p-2" type="text" id="txtAccountName" placeholder="Enter Account Name" /><br /><br />
                            <input class="p-2" type="text" id="txtAccountNumber" placeholder="Enter Account Number" onkeyup="if (/\D/g.test(this.value)) this.value = this.value.replace(/\D/g,'')" maxlength="10" /><br />
                            <img class="hidden" id="img_Roll" src="~/assets/images/gifs/loader_1.gif" />
                            <div class='link' id="place_data_request_btn"><a href='javascript:saveAndPlaceDataRequest();'><i class='fas fa-angle-right'></i></a></div>
                            <div class='light-icon'><i class='flaticon-save-money'></i></div>
                        </div>
                    </div>
                </div>
                




                <div class='col-lg-3 col-md-6 col-sm-12 pricing-block' id="nov">
                    <div class='pricing-block-one wow fadeInUp animated animated' data-wow-delay='00ms' data-wow-duration='1500ms'>

                    </div>
                </div>
            </div>
        </div>

    </div>
</section>
<!-- contact-style-two -->
<!-- contact-style-two end -->
<div id="agent_commission"></div>
<style>
    .hidden {
        display: none;
    }
</style>
<script type="text/javascript" src="/assets/st24js/AgentScript.js"></script>

<script type="text/javascript">
    if (localStorage.getItem("login_id") == null) {
        location.href = "/";
    } 
</script>
