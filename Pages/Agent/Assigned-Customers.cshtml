@page
@model ST24.Pages.Agent.Assigned_CustomersModel
@{
    ViewData["Title"] = "Assigned Customers";
}

<!-- Page Title -->
<section class="page-title" style="background-image: url(/assets/images/background/page-title.jpg);">
    <div class="auto-container">
        <div class="content-box">
            <div class="title">
                <h1>Assigned Customers</h1>
            </div>
            <ul class="bread-crumb clearfix">
                <li><a href="/Agent/dashboard">Home</a></li>
                <li>Customers</li>
            </ul>
        </div>
    </div>
</section>
<!-- End Page Title -->
<!-- process-section -->
<section class="process-section sec-pad centred">
    <div class="map-layer" style="background-image: url(/assets/images/shape/map-1.png);"></div>
    <div class="auto-container">
        <div class="row clearfix" id="dashboard">
            <div class="sec-title centred">
                <h2><span>Loading...</span></h2>
            </div>
        </div>
    </div>
</section>
<!-- chooseus-section -->
<div id="assigned_customers"></div>
<script type="text/javascript" src="/assets/st24js/AgentScript.js"></script>

<script type="text/javascript">
    if (localStorage.getItem("login_id") == null) {
        location.href = "/";
    }
</script>
